{
    "name": "nusmodifications/nuswhispers",
    "description": "NUSWhispers source. Powered by Laravel + Angular",
    "keywords": ["nuswhispers", "nusmods", "nusmodifications", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "anlutro/l4-settings": "^0.4.9",
        "bugsnag/bugsnag-laravel": "^2.14",
        "facebook/graph-sdk": "^5.6",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.1",
        "laravel/framework": "5.7.*",
        "laravel/socialite": "^3.1",
        "laravel/telescope": "^0.1.0",
        "laravelcollective/html": "5.7.*",
        "nuwave/lighthouse": "^2.6",
        "predis/predis": "^1.0",
        "sammyk/laravel-facebook-sdk": "^3.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "^2.8",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "NUSWhispers\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NUSWhispers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate",
          "@php artisan ide-helper:generate",
          "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
